                  <textLang mainLang="la">Latin</textLang>
                  <msItem>
                     <p>

   <persName>
                           <forename>John</forename> 
                           <surname type="patronymic">fitz-Philip</surname> de D<supplied>aventry</supplied>
                        </persName> grants to <persName>
                           <forename>Walter</forename> de <surname>Buckeby</surname> of <placeName role="person" key="place_108012">D<supplied>aventry</supplied>
                           </placeName>
                        </persName> 2/- rent in <placeName key="place_108012">D<supplied>aventry</supplied>
                        </placeName> from <persName>
                           <forename>Simon</forename> de <surname>aula</surname>
                        </persName>, from the tenement which Walter holds from Simon in <placeName key="place_108012">D<supplied>aventry</supplied>
                        </placeName>. Given at <placeName role="date" key="place_108012">D<supplied>aventry</supplied>
                        </placeName>. 
   Witnesses: <persName>
                           <forename>William</forename> de <surname>Drayton</surname>
                        </persName>, 
   <persName>
                           <forename>Thomas</forename> de <surname>pistrino</surname>
                        </persName>, 
   <persName>
                           <forename>Geoffrey</forename> de <surname>aula</surname>
                        </persName>, 
   <persName>
                           <forename>Robert</forename> 
                           <surname>Walies</surname>
                        </persName>, 
   <persName>
                           <forename>John</forename> 
                           <surname>Scoph</surname>
                        </persName>, Thomas ..., 
   <persName>
                           <forename>Roger</forename> 
                           <surname>Cuth</surname>
                        </persName>. 
   <origDate when="1300-12-21">
                           <supplied>21 Dec. 1300.</supplied>
                        </origDate>
                     </p>
