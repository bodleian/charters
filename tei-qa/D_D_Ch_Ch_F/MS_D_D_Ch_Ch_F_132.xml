                        <persName><forename>Peter</forename> <surname>Karbunel</surname></persName>, 
                        <persName><forename>Walter</forename> the <surname>clerk</surname> of <placeName role="person" key="place_109304">Hade</placeName></persName>, 
                        <persName><forename>William</forename> <surname>Olyver</surname></persName>, 
                        <persName><forename>William</forename> le <surname>Hunte</surname></persName>, 
                        <persName><forename>Henry</forename> <surname>Inge</surname></persName>, 
                        <persName><forename>Ralph</forename> <surname>Cucuwald</surname></persName>, 
