                        <persName><forename>John</forename> <surname>Seuri</surname></persName>, 
                        <persName><forename>Thomas</forename> <surname type="patronymic">son of Walter</surname></persName>,
                        <persName><forename>Hugh</forename> <surname>Harding</surname></persName>, 
                        <persName><forename>Adam</forename> <surname>Cruste</surname></persName>, 
                        <persName><forename>Walter</forename> the <surname>goldsmith</surname></persName>, 
                        <persName><forename>Richard</forename> the <surname>goldsmith</surname></persName>, 
                        <persName><forename>Laurence</forename> <surname type="patronymic">son of Ralph</surname></persName>, 
                        <persName><forename>Thomas</forename> <surname type="patronymic">son of Henry</surname></persName>, 
