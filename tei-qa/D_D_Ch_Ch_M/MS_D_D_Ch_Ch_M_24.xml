               <msContents>
                  <textLang mainLang="la">Latin</textLang>
                  <msItem>
                     <p>
                        <placeName key="place_107027">Clapcote</placeName>.
                             
                            <persName>
                           <forename>Walter</forename> 
                           <surname>Cirpsse</surname> of <placeName key="place_107073">W<supplied>allingford</supplied>
                           </placeName>
                        </persName> grants to <persName>
                           <forename>John</forename> ate <surname>Russche</surname>
                        </persName> of the same ½ acre of arable land in <placeName key="place_107027">C<supplied>lapcote</supplied>
                        </placeName>, in <placeName type="minor">Algareforlang</placeName>, between <persName>
                           <forename>Edmund</forename> de <surname>Bereford</surname>
                        </persName>’s land on the east and that of <persName>
                           <forename>Alice</forename> 
                           <surname>Kenelm</surname>
                        </persName> on the west, reaching from south to north over <persName>
                           <forename>William</forename> de <surname>Cornwaille</surname>
                        </persName>’s land called <placeName type="minor">Inlond</placeName>. 
                            Witnesses: <persName>
                           <forename>John</forename> 
                           <surname>Maryot</surname>
                        </persName>,
                            <persName>
                           <forename>Robert</forename> 
                           <surname>Butty</surname>
                        </persName>,
                            <persName>
                           <forename>Thomas</forename> 
                           <surname>Alayn</surname>
                        </persName>,
                            <persName>
                           <forename>William</forename> 
                           <surname>Wrouch</surname>
                        </persName>,
                            <persName>
                           <forename>Thomas</forename> 
                           <surname>Oturbourne</surname>
                        </persName>. 
                            Given at <placeName role="date" key="place_107027">C<supplied>lapcote</supplied>
                        </placeName>. 
                            <origDate when="1344-01-14">
                           <supplied>14 Jan. 1344.</supplied>
                        </origDate></p>
                  </msItem>
