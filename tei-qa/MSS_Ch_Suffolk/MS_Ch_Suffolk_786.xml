               <msContents>
                  <textLang mainLang="la">Latin</textLang>
                  <msItem>
                     <p> 
                        <placeName key="place_105176">Belton</placeName>, <placeName key="place_113106">Gapton </placeName>, <placeName key="place_105192">Bradwell</placeName>, <placeName key="place_105303">Gorleston-on-Sea</placeName>, and <placeName key="place_105295">Fritton</placeName>.
